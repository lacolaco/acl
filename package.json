    "format": "prettier --cache --write .",
    "inspect": "npx @modelcontextprotocol/inspector -- pnpm start",
    "inspect:dist": "npx @modelcontextprotocol/inspector -- node ./dist/main.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "mcp-server",
    "acl",
    "agent-communication-language"
  ],
  "author": "lacolaco <suguru.inatomi@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/lacolaco/acl.git"
  },
  "packageManager": "pnpm@10.19.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.19.1",
    "find-up": "^8.0.0"
  },
  "devDependencies": {
    "@modelcontextprotocol/inspector": "0.17.0",
    "@types/node": "^24.7.0",
    "prettier": "3.6.2",
    "tsup": "^8.5.0",
    "tsx": "4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "tsup": {
    "clean": true,
    "dts": true,
    "shims": true
  }
