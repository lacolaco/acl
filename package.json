{
  "name": "@lacolaco/acl",
  "version": "1.6.0",
  "description": "",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "main": "./dist/main.cjs",
  "module": "./dist/main.mjs",
  "types": "./dist/main.d.ts",
  "files": [
    "dist",
    "ACL.md"
  ],
  "bin": "./dist/main.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "test": "pnpm run typecheck && vitest",
    "typecheck": "tsc --noEmit",
    "start": "tsx src/main.ts",
